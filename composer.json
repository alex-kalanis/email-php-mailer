{
    "name": "alex-kalanis/email-php-mailer",
    "description": "Basic mailer in PHP for everyday use",
    "keywords": [
        "email",
        "phpmailer"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": ">=7.4.0",
        "phpmailer/phpmailer": ">=6.1",
        "alex-kalanis/email-api": "2.0"
    },
    "require-dev": {
        "shipmonk/composer-dependency-analyser": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">=7.2 <=9"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\EmailPhpMailer\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
